{
  "name": "world-id-dev-portal",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn dev:app\" \"yarn dev:kea\" -n APP,KEA -c blue,green",
    "dev:app": "next dev",
    "dev:kea": "kea-typegen watch --write-paths",
    "typegen": "kea-typegen write",
    "typecheck": "kea-typegen write && tsc",
    "build": "yarn typegen && next build",
    "start": "NODE_OPTIONS='-r next-logger' next start",
    "lint": "next lint",
    "test": "yarn test:api && yarn test:integration",
    "format": "prettier -w ./",
    "test:api": "jest tests/api",
    "test:integration": "node tests/verifyHasuraIsUp.js && jest tests/integration",
    "test:integration:server": "docker compose -f ../docker-compose-test.yaml up --abort-on-container-exit",
    "format:check": "prettier --check ./"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@ethersproject/abi": "^5.6.3",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/solidity": "^5.6.1",
    "@walletconnect/client": "^1.7.8",
    "@worldcoin/id": "^0.9.2",
    "@worldcoin/idkit": "0.3.3",
    "chart.js": "^3.9.1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "crypto-randomuuid": "^1.0.0",
    "dayjs": "^1.11.7",
    "ethers": "^5.6.8",
    "graphql": "^16.5.0",
    "jose": "^4.11.1",
    "js-sha3": "^0.8.0",
    "jsonwebtoken": "^9.0.0",
    "kea": "^3.0.1",
    "kea-forms": "^3.0.1",
    "kea-loaders": "^3.0.0",
    "next": "12.3.4",
    "next-logger": "3.0.1",
    "next-redux-wrapper": "^7.0.5",
    "posthog-js": "^1.45.1",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "17.0.2",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^14.2.1",
    "react-popper-tooltip": "^4.4.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.1.1",
    "react-useportal": "^1.0.16",
