    "@types/node": "17.0.36",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-syntax-highlighter": "^15.5.2",
    "@types/request-ip": "^0.0.37",
    "autoprefixer": "^10.4.7",
    "babel-plugin-kea": "^3.0.0",
    "concurrently": "^7.2.1",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-when": "^3.5.1",
    "kea-typegen": "^3.1.0",
